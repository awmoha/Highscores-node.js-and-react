<title>
    <%=title%>
</title>
<section class="m-1">
    <div class="container mb-3">
        <div class="row ">
            <div class="col-6 ">
                <h2>Spel</h2>
            </div>
            <div class="col-6 text-end">
                <form action="/admin/games/new">
                    <button type="submit" class="btn btn-outline-primary">Nytt spel</button>
                </form>
            </div>
        </div>
    </div>
</section>


<div class="container ">
    <table class="table table-striped table-hover  ">
        <thead>
            <tr class="table-titles ">
                <th>Id</th>
                <th>Title</th>
                <th>Genre</th>
                <th>Lanseringsår</th>
            </tr>
        </thead>
        <tbody>
            <%admin.forEach(admin => { %>
                <tr>
                    <td>
                        <%=admin.id%>
                    </td>
                    <td>
                        <%=admin.title%>
                    </td>
                    <td>
                        <%=admin.genre%>
                    </td>
                    <td>
                        <%=admin.lanseringsår%>
                    </td>
                </tr>
                <% }) %>
        </tbody>
    </table>

</div>