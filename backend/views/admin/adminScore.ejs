<title>
    <%=title%>
</title>
<h1 class="mx-5 mt-5">Nytt highscore</h1>
<form method="post"class="mx-5 mt-5">
    <div class="form-group mt-4 col-6 col-sm-4 col-md-4 col-lg-4 col-xl-4">
        <label for="exampleFormControlSelect1">spel</label>
        <select class="form-select mt-3" name="game_id" id="exampleFormControlSelect1">
            <%admin.forEach(title => { %>
                <option value="<%=title.id%>"><%=title.title%></option>
                <% }) %>
      </select>
    </div>
    <div class="form-group mt-4 col-6 col-sm-4 col-md-4 col-lg-4 col-xl-4">
        <label for="exampleFormControlInput1">First Name</label>
        <input type="text"
         name="first_name"
          class="form-control mt-3" 
          id="exampleFormControlInput1"
          placeholder="First Name">
    </div>
    <div class="form-group mt-4 col-6 col-sm-4 col-md-4 col-lg-4 col-xl-4">
        <label for="exampleFormControlInput1">Last Name</label>
        <input type="text"
         name="last_name"
          class="form-control mt-3" 
          id="exampleFormControlInput1"
          placeholder="Last Name">
    </div>
    <div class="form-group mt-4 col-6 col-sm-4 col-md-4 col-lg-4 col-xl-4">
        <label for="exampleFormControlInput1">Datum</label>
        <input type="date" 
        name="date" 
        class="form-control mt-3" 
        id="exampleFormControlInput1">
    </div>

    <div class="form-group mt-4 col-6 col-sm-4 col-md-4 col-lg-4 col-xl-4">
        <label for="exampleFormControlInput1">poäng</label>
        <input 
        type="number" 
        name="score"
         class="form-control mt-3" 
         id="exampleFormControlInput1">
    </div>
    <button type="submit" class="btn btn-primary mt-4">Lägg till</button>


</form>